<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:p="http://www.springframework.org/schema/p"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean name="st1" class="com.SpringInjector.Students">
		<property name="studentId">
			<value>101</value>
		</property>
		<property name="studentName">
			<value>Sumit Singh</value>
		</property>
		<property name="studentAdd">
			<value>Indore</value>
		</property>

	</bean>
	<bean name = "st2" class="com.SpringInjector.Students" p:studentId = "103" p:studentName = "Rahul Roy" p:studentAdd = "Ludhiana"></bean>
	
	

</beans>